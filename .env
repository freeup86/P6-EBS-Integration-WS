# Server configuration
PORT=3000
NODE_ENV=production
SESSION_SECRET=your-strong-secret-key-here

# P6 API configuration
P6_API_URL=https://your-p6-server.company.com/p6/api/v1
P6_USERNAME=your-p6-username
P6_PASSWORD=your-p6-password
P6_AUTH_TYPE=basic
P6_API_TIMEOUT=30000
P6_RETRY_ATTEMPTS=3

# EBS API configuration
EBS_API_URL=https://your-ebs-server.company.com/ebs/api
EBS_USERNAME=your-ebs-username
EBS_PASSWORD=your-ebs-password
EBS_AUTH_TYPE=basic
EBS_API_TIMEOUT=30000
EBS_RETRY_ATTEMPTS=3

# Logging
LOG_LEVEL=info